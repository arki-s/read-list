<h1>Add Progress</h1>
<%= simple_form_for(@meeting) do |f| %>
  <%= f.association :book, prompt: 'Choose a book'%>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :complete %>
  <%= f.submit "Add New Progress", class:"btn btn-primary" %>
<% end %>
<%= link_to "Back", list_path %>
