<% if @user = current_user  %>
  <h1>Edit progress</h1>
  <h2>Book Title : <%= @book.title %></h2>
  <%= simple_form_for @meeting  do |f| %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :complete %>
    <%= f.submit 'Submit', class:"btn btn-primary" %>
  <% end %>
  <%= link_to "Back", list_path %>
<% else %>
  <h1>You don't have access.</h1>
<% end %>
